nav {
    display: flex;
    align-items: center;
    /* justify-content: space-between; */
    position: fixed;
    z-index: 5;
    width: 100%;
    background-color: #88000C; /* E5E5E5 */
    /* border-bottom: black solid 1px; */
    border-top: #484848 solid 3px;
    /* visibility: hidden; */
    /* opacity: 0; */
}

nav img {
    /* flex: 1; */
    flex-shrink: 3;
    padding: 5px 20px;
    opacity: 0.9;
}

nav img:nth-of-type(2) {
    transform: rotateY(180deg);
}

nav ul:nth-of-type(1) {
    flex: 1;
    justify-content: center;
}

nav div:nth-of-type(1) {
    flex: 1;
    display: flex;
    flex-direction: column;
}

nav ul:nth-of-type(2) {
    flex: 1;
    justify-content: center;
}

nav ul {
    display: flex;
    list-style-type: none;
    margin: 0px;
    /* margin: 0px 36px; */
    padding: 0;
}

nav li {
    margin: 0px;
    padding: 10px 20px;
    height: 32px;
}

.active {
    background-color: #484848;
    color: #fff;
}

nav h6 {
    /* font-family: 'Krub', sans-serif; */
    font-family: 'Times New Roman', Times, serif;
    font-size: 22px;
    font-weight: 500;
    margin: 0;
    color: #eeeeee;
    text-align: center;
}

nav a {
    text-decoration: none;
    color: white;
    font-family: 'Noto Sans KR', sans-serif;
    font-size: 20px;
    font-weight: 400;
    letter-spacing: 1px;
}

nav a:hover {
    background-color: #808080;
    color: #fff;
}

#nav-wingding {
    height: 65px;
    width: 180px;
    background-color: #88000C;
    position: fixed;
    z-index: 4;
    left: 50%;
    transform: translateX(-50%);
    border-radius: 30px;
}

.splash {
    position: relative;
    background-color: #808080;
    background-image: url("../images/splash.jpg"); /* try align top or positioning */
    background-repeat: no-repeat;
    background-position: 50% 0%;
    background-size: cover;
    height: 100vh;
    color: #fff;
    margin: 0;
    vertical-align: top;
    display: flex;
    flex-direction:column;
    align-items:center;
    /* justify-content: center; */
    align-content: center;
    /* justify-items: center; */
}
.splash:after {
    content: " ";
    z-index: 1;
    display: block;
    position: absolute;
    height: 100%;
    top: 0;
    left: 0;
    right: 0;
    background: rgba(0, 0, 0, 0.6);
}

.splash-group {
    z-index: 2;
    display: flex;
    flex-direction: column;
    align-items: center;
    padding-top: 90px;
    margin-top: 100px;
}

