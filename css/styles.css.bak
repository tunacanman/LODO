#map-marker-transition {
    position: absolute;
    top: -80px;
}

@keyframes boing {
    0% {transform: scale(1);}
    10% {transform: scale(.9, .7);}
    20% {transform: scale(1.1, 1.3);}
    30% {transform: scale(.95, .85);}
    37% {transform: scale(1.03, 1.1);}
    44% {transform: scale(1);}
}
.animated-boing {
    animation: boing 1.5s 1;
    transform-origin: 50% 75%;
}

.location {
    position: absolute;
    display: flex;
    flex-direction:column;
    align-items:center;
    text-align: center;
}

.location h6 {
    margin: 0;
    color: #DE1426;
    font-family: 'Montserrat', sans-serif;
    font-size: 20px;
    font-weight: 600;
}

